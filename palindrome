def check():
	if h == k[::-1]:
		print("This is a palindrome")
	else:
		print("This is not a palindrome")



k = "GTAC"
h = ""
print k
for i in k: 
	if i == "A":
		h =  h + "T"
	elif i == "G":
                h =  h + "C"
	elif i == "C":
                h =  h + "G"
	else:
                h =  h + "A"

print h
check()
